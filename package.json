  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcarlosj/Aquila-WordPress-Theme/issues"
  },
  "homepage": "https://github.com/jcarlosj/Aquila-WordPress-Theme#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@beyonk/google-fonts-webpack-plugin": "^1.5.0",
    "@svgr/cli": "^5.5.0",
    "@wordpress/base-styles": "^3.3.0",
    "@wordpress/block-editor": "^5.2.0",
